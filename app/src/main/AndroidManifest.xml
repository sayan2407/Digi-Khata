<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.uit.digi_khata">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.DigiKhata">
        <activity android:name=".SignupActivity" android:parentActivityName=".LoginOrSignUp"></activity>
        <activity
            android:name=".RegisterActivity"
            android:parentActivityName=".LoginOrSignUp"></activity>
        <activity android:name=".LoginOrSignUp">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AuthenticationActivity"
            android:parentActivityName=".LoginOrSignUp"></activity>
        <activity android:name=".SplashActivity"></activity>
        <activity android:name=".MainActivity"></activity>
    </application>

</manifest>